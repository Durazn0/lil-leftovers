{
    "name": "Waterbottle",
    "description": "Toggle waterwalking",
    "type": "origins:multiple",
    "toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "key": {
            "key": "key.loadToolbarActivator"
        }
    },
    "water": {
        "type": "origins:walk_on_fluid",
        "fluid": "minecraft:water",
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        }
    },
    "lava": {
        "type": "origins:walk_on_fluid",
        "fluid": "minecraft:lava",
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        }
    }
}